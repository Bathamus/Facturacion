{
  "name": "facturacion",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8192",
  "config": {
      "blanket": {
        "pattern": "backend",
        "data-cover-never": "backend/scripts"
      }
    },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "eslint": "^3.5.0",
    "eslint-plugin-react": "^6.3.0",
    "mocha": "^3.1.2",
    "mocha-lcov-reporter": "^1.2.0",
    "react-scripts": "0.4.3"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "fixed-data-table": "^0.6.3",
    "knex": "^0.12.6",
    "material-ui": "^0.15.4",
    "react": "^15.3.2",
    "react-container-dimensions": "^1.3.0",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.6.0",
    "sqlite3": "^3.1.8",
    "superagent": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "initTestDB": "NODE_ENV=test node backend/scripts/initTestDB.js",
    "test": "react-scripts test --env=jsdom",
    "test-backend": "npm run initTestDB && NODE_ENV=test mocha",
    "travis": "npm run initTestDB && NODE_ENV=test YOURPACKAGE_COVERAGE=1 mocha --require blanket --reporter mocha-lcov-reporter | coveralls",
    "eject": "react-scripts eject"
  }
